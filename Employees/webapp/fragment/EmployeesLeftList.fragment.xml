<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
        <Panel>
            <List id="listEmployees" items="{path:'odataEmployees>/Users'}">
                <headerToolbar>
                    <OverflowToolbar>
                        <Button icon="sap-icon://nav-back" press="onBack"></Button>
                    </OverflowToolbar>
                </headerToolbar>
                <StandardListItem type="Navigation" press="onSelectEmployee"
                    title="{odataEmployees>LastName}, {odataEmployees>FirstName}"
                    description="{odataEmployees>Dni}"/>
            </List>
        </Panel>
</core:FragmentDefinition>